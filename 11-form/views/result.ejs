  <div>비밀번호은 <span><%=userInfo.password %></span>입니다.</div>
  <% if (userInfo.email) { %>
    <div>이메일은 <span><%=userInfo.email %></span>입니다.</div>
  <% } %>
  <% if (userInfo.grade) { %>
    <div>학년은 <span><%=userInfo.grade %></span>입니다.</div>
  <% } %> -->
  <script>
  </script>
</body>
</html>